   <ns2:units name="s">
      <ns2:unit units="second"/>
   </ns2:units>
   
   <ns2:units name="nM">
         <ns2:unit units="mole" prefix="nano"/>
         <ns2:unit units="liter" exponent="-1"/>
      </ns2:units>
   
   <!--
     *********************************************************
     ***       C   O   M   P   O   N   E   N   T   S       ***
     *********************************************************
     -->
   
  <ns2:component name="environment">
          <ns2:variable name="t" units="s" private_interface="yes" type="real"/>
     </ns2:component>
     
     
    <ns2:component name="geometry">
         <ns2:variable name="Ccn" units="dimensionless" private_interface="yes" type="real"/>
    <ns1:math><ns1:apply><ns1:eq/><ns1:apply><ns1:csymbol cd="cellml1">evaluatedAt</ns1:csymbol><ns1:ci>t</ns1:ci><ns1:cn ns2:units="second">0.0</ns1:cn><ns1:ci>Ccn</ns1:ci></ns1:apply><ns1:cn ns2:units="dimensionless">50.0</ns1:cn></ns1:apply></ns1:math></ns2:component>
   
      <ns2:component name="NFAT_Cycling_interface">
          <ns2:variable name="nuclearNFAT" units="nM" type="real"/>
          <ns2:variable name="NFATN_n" units="nM" public_interface="yes" type="real"/>
          <ns2:variable name="NFATp_n" units="nM" public_interface="yes" type="real"/>
   
          <ns1:math>
              <ns1:apply>
                      <ns1:eq/>
                      <ns1:ci>nuclearNFAT</ns1:ci>
                      <ns1:apply>
                         <ns1:plus/>
                         <ns1:ci>NFATN_n</ns1:ci>
                         <ns1:ci>NFATp_n</ns1:ci>
                      </ns1:apply>
            </ns1:apply>
          </ns1:math>
   </ns2:component>
   
   
<ns2:encapsulation><ns2:component_ref component="geometry">
                <ns2:component_ref component="calcium"/>
                <ns2:component_ref component="calcineurin"/>
                <ns2:component_ref component="NFAT_Cycling"/>
                <ns2:component_ref component="NFAT_Cycling_interface"/>
             </ns2:component_ref></ns2:encapsulation>   

   
<ns2:connection component_2="calcium" component_1="environment">
    
    <ns2:map_variables variable_1="t" variable_2="t"/>
</ns2:connection>

